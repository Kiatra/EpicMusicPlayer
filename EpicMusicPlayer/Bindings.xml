<Bindings>
	<Binding name="PLAYSTOP" header="EPICMUSICPLAYER">
		EpicMusicPlayer:TogglePlay()
	</Binding>
	<Binding name="NEXT">
		EpicMusicPlayer:PlayNext()
	</Binding>
	<Binding name="LAST">
		EpicMusicPlayer:PlayLast()
	</Binding>
	<Binding name="MUTE">
		EpicMusicPlayer:ToggleMute()
	</Binding>
	<Binding name="REMOVESONG">
		if(EpicMusicPlayer.Playing)then
			EpicMusicPlayer:RemoveSong(EpicMusicPlayer.db.profile.list,EpicMusicPlayer.db.profile.song);
		else
			EpicMusicPlayer:Print("Not playing.")
		end
	</Binding>
	<Binding name="GUI">
		EpicMusicPlayer:ToggleGui()
	</Binding>
	<Binding name="TOGGLELIST">
		EpicMusicPlayer:TogglePlaylist()
	</Binding>
	<Binding name="BADLIST">
		EpicMusicPlayer:ToBadList()
	</Binding>
	<Binding name="BESTLIST">
		EpicMusicPlayer:ToBestList()
	</Binding>
</Bindings>
