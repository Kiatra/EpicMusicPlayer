<Bindings>
	<Binding name="PLAYSTOP" header="EPICMUSICPLAYER">
		LibStub("AceAddon-3.0"):GetAddon("EpicMusicPlayer"):TogglePlay()
	</Binding>
	<Binding name="NEXT">
		LibStub("AceAddon-3.0"):GetAddon("EpicMusicPlayer"):PlayNext()
	</Binding>
	<Binding name="LAST">
		EpicMusicPlayer = LibStub("AceAddon-3.0"):GetAddon("EpicMusicPlayer"):PlayLast()
	</Binding>
	<Binding name="MUTE">
		EpicMusicPlayer = LibStub("AceAddon-3.0"):GetAddon("EpicMusicPlayer"):ToggleMute()
	</Binding>
	<Binding name="REMOVESONG">
		EpicMusicPlayer = LibStub("AceAddon-3.0"):GetAddon("EpicMusicPlayer")
		if(EpicMusicPlayer.Playing)then
			EpicMusicPlayer:RemoveCurrendSong();
		else
			EpicMusicPlayer:Print("Not playing.")
		end
	</Binding>
	<Binding name="GUI">
		EpicMusicPlayer = LibStub("AceAddon-3.0"):GetAddon("EpicMusicPlayer"):ToggleGui()
	</Binding>
	<Binding name="TOGGLELIST">
		EpicMusicPlayer = LibStub("AceAddon-3.0"):GetAddon("EpicMusicPlayer"):TogglePlaylist()
	</Binding>
</Bindings>
